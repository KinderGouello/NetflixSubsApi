---
  extends: "airbnb"
  env:
    node: true
  rules:
    new-cap:
      - "error"
      - capIsNewExceptions:
          - "express.Router"
    import/no-dynamic-require: 0
    arrow-parens: 0
    comma-dangle: [2, "always-multiline"]
    no-plusplus:
    - error
    - allowForLoopAfterthoughts: true
